params {
  input = "$launchDir/input/"
  samplesheetFileName = "samplesheet.csv"
  genome = "$launchDir/input/genome.fasta"
  experimentType = "originsOfReplication"
  referenceSample = "G2"
  hasPairedReads = true
  removePCRDuplicates = true
  outputDir = "$launchDir/output"
  inputFileType = "fastq"
}


process {

    withName: bowtie2 {
        ext.args = ''
    }
}

docker {
    enabled = true
}
